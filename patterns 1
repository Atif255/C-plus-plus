#include<iostream>
using namespace std;
int main() {
  int n;
  cout << "enter the number of rows";            
  cin >> n;
  int i =1;
  while (i<=n) {
    int j = 1;
    while (j<=n){
      cout << "*"; 
      j = j+1;
    }
    i = i+1;
    cout << endl;
    
  }
}  



#include<iostream>
using namespace std;
int main() {
  int n;
  cout << "enter the number of rows";
  cin >> n;
  int i =1;
  while(i<=n) {
    int j =1;
    while (j<=n) {
      cout << i;
      j = j+1;
    }
    i = i+1;
    cout<<endl;
  }
}


int n;
  cin >> n;
  int i =1;
  while (i<=n){
    int j =1;
    while(j<=n){
      cout << j;
      j =j+1;
      }
    i=i+1;
    cout << endl;
  }
  
  
  int n;
  cin >> n;
  int i =1;
  int count = 1; 
  while (i<=n){
    int j =1;
    while (j<=n) {
      cout << count;
      count = count+1;
      j = j+1;
    }
    i = i+1;
    cout << endl;
  }
  
  
  int n;
  cin >> n;
  int i = 1;
  while (i<=n) {
    int j =1;
    while (j<=i){
      cout << "*";
      j = j+1;
    }
    i = i+1;
    cout << endl; 
  }
  
  
  while (i<=n) {
    int j =1;
